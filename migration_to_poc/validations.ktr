    <sql>select 
(select concat(count(*),' person_poc') from person)  as nr_person, 
(select concat(count(*),' patient_poc') from patient) as nr_patients,
(select concat(count(*),' names_poc') from person_name) as nr_person_names,
(select concat(count(*),' address_poc') from person_address) as nr_person_add,

(select concat(count(*),' visits_epts') FROM visit where visit_type_id in ('3','5','8','10','12')) as nr_visits,

(select concat(count(*),' encounter_poc') from encounter where encounter_type in ('1','8')) as nr_encounters,
(select concat(count(*),' obs_poc') from obs) as nr_obs,
(select concat(count(*),' meds_poc') from orders where order_type_id='2') as nr_meds,
(select concat(count(*),' lab_order_poc') from orders where order_type_id='3') as nr_laborders,
 from dual;</sql>
