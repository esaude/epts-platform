    <connection>epts</connection>
    <sql>select 
(select count(*) from person p inner join patient pt on p.person_id = pt.patient_id where p.voided=0 and pt.voided = 0)  as reg_pat, 
(select count(*) from person p inner join patient pt on p.person_id = pt.patient_id where p.voided=0 and pt.voided = 0 and p.gender = 'M') as pat_male,
(select count(*) from person p inner join patient pt on p.person_id = pt.patient_id where p.voided=0 and pt.voided = 0 and p.gender = 'F') as pt_fem,
(select count(*) from person p inner join patient pt on p.person_id = pt.patient_id where p.voided=0 and pt.voided = 0 
and year(p.birthdate) > (year(now())-5) and p.birthdate is not null) as pat_under_five,

(select count(*) from person p inner join patient pt on p.person_id = pt.patient_id where p.voided=0 and pt.voided = 0 
and year(p.birthdate) between (year(now())-14) and (year(now())-5) and p.birthdate is not null) as pat_five_fifteen,

(select count(*) from person p inner join patient pt on p.person_id = pt.patient_id where p.voided=0 and pt.voided = 0 
and year(p.birthdate) between year(now())-45 and year(now())-16 and p.birthdate is not null) as pat_sixteen_fourtysix,

(select count(*) from person p inner join patient pt on p.person_id = pt.patient_id where p.voided=0 and pt.voided = 0 
and year(p.birthdate) &lt; (year(now())-45) and p.birthdate is not null) as pat_over_fourtysix,

(select count(*) from person p 
inner join patient pt on p.person_id = pt.patient_id
inner join patient_identifier pa on p.person_id = pa.patient_id
where p.voided=0 and pt.voided = 0 and pa.identifier_type = 3) as pat_bi,

(select distinct count(*),case 
when ob.concept_id = 1740 then 'NOME CONFIDENTE'
when ob.concept_id = 1611 then 'CONTACTO CONFIDENTE'
else 'other'
end as dados
,ob.concept_id 
from person p 
inner join patient pt on p.person_id = pt.patient_id
inner join obs ob on p.person_id = ob.person_id
where p.voided=0 and pt.voided = 0 and ob.concept_id in (1740,1611)
group by ob.concept_id) as pat_conf,

(select 
case
when programs.program_id = 1 then 'pre tarv'
when programs.program_id = 2 then 'tarv'
end as program, count(programs.patient_id)
from (select
distinct p.patient_id,program.program_id,max(program.state_id)
from patient p
inner join 
(select pp.patient_id, pp.patient_program_id,state.state_id, program_id
from patient_program pp 
inner join 
(select patient_program_id, max(patient_state_id) state_id from patient_state group by patient_program_id) state on state.patient_program_id = pp.patient_program_id 
where pp.program_id in (1,2) and pp.voided=0 and pp.date_completed is null) program on program.patient_id = p.patient_id
inner join (select state,patient_state_id from patient_state) the_state on the_state.patient_state_id = program.state_id
where p.voided=0 
group by p.patient_id,program.program_id) as programs
group by program
) as pat_tarv_status,

(select count(*) from 
person p
inner join CCSDBMASKED.patient pp on p.person_id =  pp.patient_id
inner join CCSDBMASKED.obs ob on p.person_id = ob.person_id
where ob.concept_id = 6257 AND ob.value_coded = 1065) as pat_tb ,


() as ;
















 from dual</sql>
